{
    "aws_service": "iam",
    "gcp_service": "iam",
    "mappings": [
        {
            "aws": "boto3.client('iam')",
            "gcp": "iam_admin_v1.IAMClient()",
            "imports": [
                "from google.cloud import iam_admin_v1"
            ]
        }
    ],
    "terraform_mappings": {
        "aws_iam_role": "google_service_account",
        "aws_iam_policy": "google_project_iam_custom_role",
        "aws_iam_role_policy_attachment": "google_project_iam_binding"
    }
}