{
    "aws_service": "s3",
    "gcp_service": "storage",
    "mappings": [
        {
            "aws": "regex:boto3\\.client\\('s3'.*?\\)",
            "gcp": "storage.Client()",
            "imports": [
                "from google.cloud import storage"
            ]
        },
        {
            "aws": "s3.put_object",
            "gcp": "bucket.blob(key).upload_from_string(body)"
        },
        {
            "aws": "s3.upload_file",
            "gcp": "bucket.blob(key).upload_from_filename(filename)"
        },
        {
            "aws": "regex:self\\.s3_client\\.upload_file\\(([^,]+),\\s*([^,]+),\\s*([^)]+)\\)",
            "gcp": "self.bucket.blob(\\3).upload_from_filename(\\1)"
        },
        {
            "aws": "regex:([a-zA-Z0-9_]+)\\.put_bucket_notification_configuration\\(.*?\\)",
            "gcp": "# [Action Required] Configure GCS Pub/Sub notifications: gsutil notification create -t [TOPIC] -f json gs://[BUCKET]"
        }
    ],
    "terraform_mappings": {
        "aws_s3_bucket": "google_storage_bucket"
    }
}